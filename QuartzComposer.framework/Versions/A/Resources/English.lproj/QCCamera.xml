<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	
	<key>nodeAttributes</key>
	<dict>
		<key>copyright</key>
		<string>Apple Computer, Inc. 2003-2004</string>
		<key>description</key>
		<string>This macro patch applies a 3D transformation to its subpatches: a 3 angles rotation around an origin point, followed by a 3 axes translation.</string>
		<key>name</key>
		<string>3D Transformation</string>
		<key>category</key>
		<string>Environment</string>
	</dict>
	
	<key>inputAttributes</key>
	<dict>
		<key>inputOriginX</key>
		<dict>
			<key>description</key>
			<string>Rotation origin on the X-axis.</string>
			<key>name</key>
			<string>Rotation Origin X</string>
		</dict>
		<key>inputOriginY</key>
		<dict>
			<key>description</key>
			<string>Rotation origin on the Y-axis.</string>
			<key>name</key>
			<string>Rotation Origin Y</string>
		</dict>
		<key>inputOriginZ</key>
		<dict>
			<key>description</key>
			<string>Rotation origin on the Z-axis.</string>
			<key>name</key>
			<string>Rotation Origin Z</string>
		</dict>
		<key>inputRotateX</key>
		<dict>
			<key>description</key>
			<string>Rotation on the X-axis.</string>
			<key>name</key>
			<string>Rotation X</string>
		</dict>
		<key>inputRotateY</key>
		<dict>
			<key>description</key>
			<string>Rotation on the Y-axis.</string>
			<key>name</key>
			<string>Rotation Y</string>
		</dict>
		<key>inputRotateZ</key>
		<dict>
			<key>description</key>
			<string>Rotation on the Z-axis.</string>
			<key>name</key>
			<string>Rotation Z</string>
		</dict>
		<key>inputTranslateX</key>
		<dict>
			<key>description</key>
			<string>Translation amount on the X-axis.</string>
			<key>name</key>
			<string>Translation X</string>
		</dict>
		<key>inputTranslateY</key>
		<dict>
			<key>description</key>
			<string>Translation amount on the Y-axis.</string>
			<key>name</key>
			<string>Translation Y</string>
		</dict>
		<key>inputTranslateZ</key>
		<dict>
			<key>description</key>
			<string>Translation amount on the Z-axis.</string>
			<key>name</key>
			<string>Translation Z</string>
		</dict>
	</dict>
	
</dict>
</plist>
