<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	
	<key>nodeAttributes</key>
	<dict>
		<key>copyright</key>
		<string>Apple Computer, Inc. 2003-2004</string>
		<key>category</key>
		<string>- Private</string>
		<key>description</key>
		<string>Mixes two colors together.</string>
		<key>name</key>
		<string>Color Mixer</string>
	</dict>
	
	<key>inputAttributes</key>
	<dict>
		<key>inputColor1</key>
		<dict>
			<key>description</key>
			<string>The first color.</string>
			<key>name</key>
			<string>First color</string>
		</dict>
		<key>inputColor2</key>
		<dict>
			<key>description</key>
			<string>The second color.</string>
			<key>name</key>
			<string>Second color</string>
		</dict>
		<key>inputMix</key>
		<dict>
			<key>description</key>
			<string>The proportional quantities of the two colors to mix.</string>
			<key>name</key>
			<string>Mixing Point</string>
		</dict>
		<key>inputMode</key>
		<dict>
			<key>description</key>
			<string>The mixing mode to use.</string>
			<key>name</key>
			<string>Mixing Mode</string>
			<key>menu</key>
			<array>
				<string>RGB</string>
				<string>HSL</string>
			</array>
		</dict>
	</dict>
	
	<key>outputAttributes</key>
	<dict>
		<key>outputColor</key>
		<dict>
			<key>description</key>
			<string>The resulting color.</string>
			<key>name</key>
			<string>Color</string>
		</dict>
	</dict>
	
</dict>
</plist>
