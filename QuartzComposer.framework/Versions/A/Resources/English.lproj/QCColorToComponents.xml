<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	
	<key>.identifiers</key>
	<dict>
		
		<key>rgb</key>
		<dict>
			<key>nodeAttributes</key>
			<dict>
				<key>description</key>
				<string>Extracts the red, green, blue, and alpha components from a color.</string>
				<key>name</key>
				<string>Color to RGB</string>
			</dict>
			<key>outputAttributes</key>
			<dict>
				<key>output1</key>
				<dict>
					<key>description</key>
					<string>The red component.</string>
					<key>name</key>
					<string>Red</string>
				</dict>
				<key>output2</key>
				<dict>
					<key>description</key>
					<string>The green component.</string>
					<key>name</key>
					<string>Green</string>
				</dict>
				<key>output3</key>
				<dict>
					<key>description</key>
					<string>The blue component.</string>
					<key>name</key>
					<string>Blue</string>
				</dict>
			</dict>
		</dict>
		
		<key>hsl</key>
		<dict>
			<key>nodeAttributes</key>
			<dict>
				<key>description</key>
				<string>Extracts the hue, saturation, luminosity, and alpha components from a color.</string>
				<key>name</key>
				<string>Color to HSL</string>
			</dict>
			<key>outputAttributes</key>
			<dict>
				<key>output1</key>
				<dict>
					<key>description</key>
					<string>The hue component.</string>
					<key>name</key>
					<string>Hue</string>
				</dict>
				<key>output2</key>
				<dict>
					<key>description</key>
					<string>The saturation component.</string>
					<key>name</key>
					<string>Saturation</string>
				</dict>
				<key>output3</key>
				<dict>
					<key>description</key>
					<string>The luminosity component.</string>
					<key>name</key>
					<string>Luminosity</string>
				</dict>
			</dict>
		</dict>
		
	</dict>
	
	<key>nodeAttributes</key>
	<dict>
		<key>copyright</key>
		<string>Apple Computer, Inc. 2003-2004</string>
		<key>category</key>
		<string>Modifier</string>
	</dict>
	
	<key>inputAttributes</key>
	<dict>
		<key>inputColor</key>
		<dict>
			<key>description</key>
			<string>The source color.</string>
			<key>name</key>
			<string>Color</string>
		</dict>
	</dict>
	
	<key>outputAttributes</key>
	<dict>
		<key>outputAlpha</key>
		<dict>
			<key>description</key>
			<string>The alpha component.</string>
			<key>name</key>
			<string>Alpha</string>
		</dict>
	</dict>
	
</dict>
</plist>
